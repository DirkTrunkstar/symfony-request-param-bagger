{
    "name": "php-arsenal/symfony-request-param-bagger",
    "description": "Collects sent parameters from Symfony Request and outputs an array.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PhpArsenal\\SymfonyRequestParamBagger\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0.0",
        "symfony/http-foundation": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "run-tests": "vendor/bin/phpunit --no-coverage",
        "run-coverage": "vendor/bin/phpunit --coverage-clover tests/_output/coverage.xml",
        "run-coverage-text": "vendor/bin/phpunit --coverage-text=tests/_output/coverage.txt && cat tests/_output/coverage.txt"
    }
}
